        "@types/jest": "^26.0.24",
        "@types/testing-library__jest-dom": "^5.14.3",
        "@types/lodash": "^4.14.171",
        "esbuild-copy-static-files": "^0.1.0",
        "esbuild-plugin-inline-image": "^0.0.8",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.5.1",
        "ts-jest": "^27.1.3",
        "tsup": "^5.11.13",
        "typescript": "^4.7.4",
        "nock": "^13.1.3"
